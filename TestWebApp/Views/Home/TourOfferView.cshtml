@model TourOffer

<form asp-controller="Home" asp-action="SaveOfferTour" method="post">

    <div class="form-group">
        <label asp-for="id">id</label>
        <input asp-for="id" value="@Model.id" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="type">type</label>
        <input asp-for="type" value="@Model.type" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="bid">bid</label>
        <input asp-for="bid" value="@Model.bid" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="available">available</label>
        <input asp-for="available" value="@Model.available" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="url">url</label>
        <input asp-for="url" value="@Model.url" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="price">price</label>
