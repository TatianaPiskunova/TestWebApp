        <input asp-for="categoryId" value="@Model.categoryId" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="categoryIdType">categoryIdType</label>
        <input asp-for="categoryIdType" value="@Model.categoryIdType" class="form-control" readonly />
