        <input asp-for="region" value="@Model.region" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="days">days</label>
        <input asp-for="days" value="@Model.days" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="dataTour">dataTour</label>
        <input asp-for="dataTour" value="@Model.dataTour" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="name">name</label>
        <input asp-for="name" value="@Model.name" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="hotelStars">hotelStars</label>
