        <input asp-for="included" value="@Model.included" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="transport">transport</label>
        <input asp-for="transport" value="@Model.transport" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="description">description</label>
        <input asp-for="description" value="@Model.description" class="form-control" readonly />
    </div>

    <div class="col d-grid">
        <input type="submit" class="btn btn-primary m-2" value="Сохранить в Базу Данных" />
    </div>
</form>
<div class="col d-grid">
    <a asp-controller="Home" asp-action="Index" class="btn btn-primary m-2">Назад</a>
</div>

           